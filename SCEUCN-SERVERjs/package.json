  },
  "homepage": "https://github.com/ChristianFarias23/SCE-UCN#readme",
  "devDependencies": {
    "@types/node": "^12.0.3",
    "prettier": "^1.17.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
